<table>
    <thead>
        <tr>
            <td>#</td>
            <td>Title</td>
            <td>Released at</td>
        </tr>
    </thead>
    <tbody>
    <% @albums.each do |album| %>
        <tr>
            <td><%= album.id %></td>
            <td><%= album.title %></td>
            <td><%= album.released_at %></td>
        </tr>
    <% end %>
    </tbody>
</table>